<tr>
	<td><%= client_exception_counter + 1 %></td>
	<td><%= client_exception.client_version %></td>
	<td><%= client_exception.num %></td>
	<td>
		<%=link_to client_exception do %>
			<%= client_exception.md5%>

		<% end %>
	</td>
	<td><%= client_exception.created_at %></td>
	<td>
		<%= render partial:'share/edit',  locals:{edit_url:[:edit,client_exception]} %>
		<%= render partial:'share/delete',locals:{delete_url:[client_exception]} %>
	</td>
</tr>