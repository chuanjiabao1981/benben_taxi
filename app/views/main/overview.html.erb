<div class="row-fluid">
	<div class="infobox-container">
		<% @infos.each do |info| %>
			<div class="infobox <%= info[:color]%>  ">
				<div class="infobox-icon">
					<i class="<%= info[:icon]%>"></i>
				</div>
				<div class="infobox-data">
					<span class="infobox-data-number"><%= info[:num]%></span>
					<div class="infobox-content"><%= info[:content]%></div>
				</div>
				<div class="stat stat-success">8%</div>
			</div>
		<% end %>
	</div>
</div>

